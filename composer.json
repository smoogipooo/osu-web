{
  "name": "ppy/osu-web",
  "description": "osu!web",
  "type": "project",
  "homepage": "https://new.ppy.sh",
  "license": "AGPL-3.0",
  "support": {
    "email": "support@ppy.sh",
    "issues": "https://github.com/ppy/osu-web/issues"
  },
  "require": {
    "anhskohbo/no-captcha": "^3.2",
    "chaseconey/laravel-datadog-helper": ">=1.2.0",
    "doctrine/dbal": "^2.0",
    "egulias/email-validator": "*",
    "elasticsearch/elasticsearch": "^7.12.0",
    "ezyang/htmlpurifier": "*",
    "firebase/php-jwt": "^5.2",
    "graham-campbell/github": "*",
    "guzzlehttp/guzzle": "*",
    "itsgoingd/clockwork": "dev-master#fd0e893",
    "jenssegers/agent": "*",
    "knuckleswtf/scribe": "^2.7",
    "laravel/framework": "^6.20.14",
    "laravel/helpers": "^1.1",
    "laravel/passport": "*",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/tinker": "*",
    "laravelcollective/html": "*",
    "league/commonmark": "^1.6",
    "league/flysystem-aws-s3-v3": "*",
    "league/fractal": "*",
    "mariuzzo/laravel-js-localization": "*",
    "paypal/rest-api-sdk-php": "*",
    "php-ds/php-ds": "^1.3",
    "sentry/sentry-laravel": "*",
    "symfony/yaml": "*",
    "tightenco/ziggy": ">=0.8.1",
    "xsolla/xsolla-sdk-php": ">=4.2.0"
  },
  "require-dev": {
    "beyondcode/laravel-query-detector": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
    "dms/phpunit-arraysubset-asserts": "^0.2.1",
    "facade/ignition": "^1.0",
    "fzaninotto/faker": "*",
    "laravel/dusk": "*",
    "mockery/mockery": "*",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": ">=9.5",
    "slevomat/coding-standard": "^6.4",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/css-selector": "*",
    "symfony/dom-crawler": "*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.0"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/passport"
      ]
    }
  }
}
